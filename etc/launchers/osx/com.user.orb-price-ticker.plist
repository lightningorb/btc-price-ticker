<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.orb-price-ticker</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>HOME_DIR/bin/orb_price_ticker/orb_price_ticker_server</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>/tmp/com.user.wsb.stdout</string>
    
    <key>StandardErrorPath</key>
    <string>/tmp/com.user.wsb.stderr</string>
    
    <key>KeepAlive</key>
    <true/>
</dict>
</plist>
